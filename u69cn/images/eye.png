                timer = setInterval(slide, 3000);
                //$('.blk_jdt_nextbtn').hide();
                //$('.blk_jdt_prebtn').hide();
            };


            oNext.onclick = function () {
                slide();
                return false;
            }

            oPrev.onclick = function () {
                var left = oUl.offsetLeft;
                var index = Math.ceil(-left / 320);
                var num = Math.ceil(oUl.offsetWidth / 320);
                if (index == 0) {
                    index = num - 1;
                } else {
                    index -= 1;
                }
                oUl.style.left = -index * 320 + "px";

                return false;
            }

            function slide() {
                var i = 0;
                var left = oUl.offsetLeft;
                var index = Math.ceil(-left / 320);
                var num = Math.ceil(oUl.offsetWidth / 320);
                if (index >= (num - 1)) {
                    index = 0;
                } else {
                    index += 1;
                }
                oUl.style.left = -index * 320 + "px";

            }
            YlMarqueeFn('scroll-verticale');

            slideFunc({
                "oDiv": $('#slide_quanqiu_wrap')[0],
